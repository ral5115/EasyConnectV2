                                    Width="9"
                                    Height="9"
                                    Visibility="{Binding IsMouseOver, Converter={StaticResource BooleanToVisibilityConverter},ElementName=MoveToFirstPageButton}"
                                    SnapsToDevicePixels="True">
                                <Path Data="M4,0L4,7 0,3.35z" Fill="{telerik:VisualStudio2013Resource ResourceKey=MarkerBrush}" Stretch="Fill" Width="7" Height="9"/>
                                <Path Data="M0,0 L2,0 L2,9 L0,9 z" Fill="{telerik:VisualStudio2013Resource ResourceKey=MarkerBrush}" Height="9" Margin="0,0,7,0" Stretch="Fill" Width="2"/>
                            </Grid>
