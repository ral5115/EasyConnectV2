                    <telerik:RadButton
                            Name="MoveToNextPageButton"
                            Visibility="{Binding PagerControlsVisibility.MoveToNextPageControlVisibility,RelativeSource={RelativeSource TemplatedParent}}"
                            Style="{StaticResource PageButtonStyle}"
                            Command="{x:Static controls:RadDataPagerCommands.MoveToNextPage}">
                        <Grid>
                            <Path
                                    Data="M4,0L4,7 0,3.35z"
                                    RenderTransformOrigin="0.5,0.5"
                                    Fill="{telerik:VisualStudio2013Resource ResourceKey=StrongBrush}"
                                    HorizontalAlignment="Center"
                                    Stretch="Fill"
                                    Width="7"
                                    Height="9"
                                    VerticalAlignment="Center"
                                    SnapsToDevicePixels="True">
                                <Path.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="-1"/>
                                        <SkewTransform/>
                                        <RotateTransform/>
                                        <TranslateTransform/>
                                    </TransformGroup>
                                </Path.RenderTransform>
                            </Path>
                            <Path
                                    Data="M4,0L4,7 0,3.35z"
                                    RenderTransformOrigin="0.5,0.5"
                                    Fill="{telerik:VisualStudio2013Resource ResourceKey=MarkerBrush}"
                                    Visibility="{Binding IsMouseOver, Converter={StaticResource BooleanToVisibilityConverter}, ElementName=MoveToNextPageButton}"
                                    HorizontalAlignment="Center"
                                    Stretch="Fill"
                                    Width="7"
                                    Height="9"
                                    VerticalAlignment="Center"
                                    SnapsToDevicePixels="True">
                                <Path.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="-1"/>
                                        <SkewTransform/>
                                        <RotateTransform/>
                                        <TranslateTransform/>
                                    </TransformGroup>
                                </Path.RenderTransform>
                            </Path>
                            <Path
                                    Data="M4,0L4,7 0,3.35z"
                                    RenderTransformOrigin="0.5,0.5"
                                    Fill="{telerik:VisualStudio2013Resource ResourceKey=SelectedBrush}"
                                    Visibility="{Binding IsPressed, Converter={StaticResource BooleanToVisibilityConverter}, ElementName=MoveToNextPageButton}"
                                    HorizontalAlignment="Center"
                                    Stretch="Fill"
                                    Width="7"
                                    Height="9"
                                    VerticalAlignment="Center"
                                    SnapsToDevicePixels="True">
                                <Path.RenderTransform>
                                    <TransformGroup>
                                        <ScaleTransform ScaleX="-1"/>
                                        <SkewTransform/>
                                        <RotateTransform/>
                                        <TranslateTransform/>
                                    </TransformGroup>
                                </Path.RenderTransform>
                            </Path>
